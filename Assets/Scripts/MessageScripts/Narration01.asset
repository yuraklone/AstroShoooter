  msgArray:
  - name: "\u30CA\u30EC\u30FC\u30B7\u30E7\u30F3"
    message: "\u2015\u2015\u9065\u304B\u9060\u3044\u672A\u6765\u3001\u661F\u306E\u6226\u4E89\u304C\u7D42\u308F\u3063\u305F\u591C\u3002\n\u6EC5\u3073\u3086\u304F\u60D1\u661F\u304B\u3089\u547D\u304B\u3089\u304C\u3089\u8131\u51FA\u3057\u305F\u3068\u3042\u308B\u9B54\u5973\u306F\u3001\n\u5D29\u58CA\u5BF8\u524D\u306E\u5B87\u5B99\u8239\u306B\u4E00\u4EBA\u53D6\u308A\u6B8B\u3055\u308C\u3066\u3044\u305F\u3002"
  - name: "\u30CA\u30EC\u30FC\u30B7\u30E7\u30F3"
    message: "\u610F\u8B58\u3092\u5931\u3063\u305F\u307E\u307E\u3001\u51B7\u51CD\u7761\u7720\u30DD\u30C3\u30C9\u3067\u4F55\u767E\u5E74\u3082\u6F02\u6D41\u3057\u3001\n\u5F7C\u5973\u304C\u76EE\u899A\u3081\u305F\u306E\u306F\u3001\u6B63\u4F53\u4E0D\u660E\u306E\u5EC3\u68C4\u5B87\u5B99\u8239\u306E\u4E2D\u2015\u2015\n\n\u8239\u5185\u306B\u306F\u8AB0\u3082\u3044\u306A\u3044\u3002\n\u7A7A\u6C17\u306F\u308F\u305A\u304B\u306B\u6B8B\u308A\u3001\u8B66\u544A\u97F3\u304C\u9759\u304B\u306B\u9CF4\u308A\u97FF\u304F\u3002"
  - name: "\u30CA\u30EC\u30FC\u30B7\u30E7\u30F3"
    message: "\u304B\u3064\u3066\u306E\u9B54\u6CD5\u3082\u3001\u4ECA\u306F\u3082\u3046\u306A\u3044\u3002\n\u306A\u305C\u3053\u3053\u306B\u3044\u308B\u306E\u304B\uFF1F\n\u3053\u306E\u8239\u306F\u3069\u3053\u304B\u3089\u6765\u3066\u3001\u3069\u3053\u3078\u5411\u304B\u3046\u306E\u304B\uFF1F\n\n\u300C\u2026\u3053\u3053\u306B\u3044\u3066\u3082\u4ED5\u65B9\u304C\u306A\u3044\u300D\n\u90E8\u5C4B\u306B\u3042\u308B\u8AB0\u306E\u3082\u306E\u304B\u3082\u308F\u304B\u3089\u306A\u3044\u9283\u3092\u624B\u306B\u53D6\u308A\n\u6697\u95C7\u306E\u4E2D\u3001\u5F7C\u5973\u306F\u6B69\u304D\u51FA\u3057\u305F\u3002"
